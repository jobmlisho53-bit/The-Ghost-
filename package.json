{
  "name": "ghost-youtube",
  "version": "1.0.0",
  "description": "AI-native video platform with on-demand content generation",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "node backend/src/app.js",
    "test": "jest",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "ai",
    "video",
    "content-generation",
    "youtube",
    "on-demand"
  ],
  "author": "Ghost YouTube Team",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2"
  }
}
